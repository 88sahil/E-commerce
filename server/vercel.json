{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "methods":["GET","PUT","POST","PATCH","OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": ["https://mpfstore.vercel.app","http://localhost:5173"],
                "Access-Control-Allow-Credentials": "true"
            }
        }
    ]
}